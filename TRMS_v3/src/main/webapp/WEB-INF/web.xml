<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>servletSector.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>servletSector.LoginServlet</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>LogOutServlet</servlet-name>
  	<display-name>LogOutServlet</display-name>
  	<description></description>
  	<servlet-class>servletSector.LogOutServlet</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>ProfileServlet</servlet-name>
  	<display-name>ProfileServlet</display-name>
  	<description></description>
  	<servlet-class>servletSector.ProfileServlet</servlet-class>
  </servlet>
    <servlet>
    	<servlet-name>SubmitReimbursementServlet</servlet-name>
    	<display-name>SubmitReimbursementServlet</display-name>
    	<description></description>
    	<servlet-class>servletSector.SubmitReimbursementServlet</servlet-class>
    </servlet>

  
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>LogOutServlet</servlet-name>
  	<url-pattern>/LogOutServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>ProfileServlet</servlet-name>
  	<url-pattern>/ProfileServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>SubmitReimbursementServlet</servlet-name>
   	<url-pattern>/SubmitReimbursementServlet</url-pattern>
   </servlet-mapping>
  
  <welcome-file-list>
  <!-- If a particular webpage isn't specified, server will run this; priority over index.html, index.jsp -->
   <welcome-file>mainPage.html</welcome-file>
  </welcome-file-list>
  
  
</web-app>
